[application]

config/name="January"
run/main_scene="res://areas/test_area.tscn"
config/icon="res://icon.png"

[autoload]

random="res://engine/random.gd"
veiwport_scaler="res://engine/veiwport_scaler.gd"
directions="*res://engine/directions.gd"
layer_shifter="*res://engine/layer_shifter.gd"

[debug]

gdscript/warnings/unused_class_variable=false

[display]

window/size/width=320
window/size/height=180
window/size/test_width=960
window/size/test_height=540
window/dpi/allow_hidpi=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

